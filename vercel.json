{
    "version": 2,
    "rewrites": [
        { 
            "source": "/api/(.*)", 
            "destination": "/api" 
        }
    ],
    "builds": [
        {
            "src": "app.js", 
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}